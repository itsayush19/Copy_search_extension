{
    "name":"Copy and Search",
    "description":"Allows you to copy a text and search it on a search engine",
    "version": "1.0",
    "manifest_version":2,
    "background":{
        "scripts": ["background.js"]
    },
    "browser_action":{
        "default_popup":"popup.html"
    },
    "permissions":[
        "tabs",
        "<all_urls>"
    ]
}